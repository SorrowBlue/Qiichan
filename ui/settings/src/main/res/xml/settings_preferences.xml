<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto">
	<PreferenceCategory app:title="UI">
		<ListPreference
			android:defaultValue="@string/settings_label_follow_system"
			android:entries="@array/settings_dark_mode"
			android:entryValues="@array/settings_dark_mode_value"
			android:key="@string/settings_key_dark_mode"
			android:title="@string/settings_label_dark_mode"
			app:useSimpleSummaryProvider="true" />
		<Preference
			android:key="login"
			app:title="@string/settings_label_login" />
		<Preference
			android:key="logout"
			app:title="@string/settings_label_logout" />
	</PreferenceCategory>
</PreferenceScreen>
